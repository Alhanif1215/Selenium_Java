package string.programming;

public class ToUpperCase {
	public static void main(String[] args) {
		String str="He110 $ Duniya";
		String upStr="";
		for (int i = 0; i < str.length(); i++) {
			char ch=str.charAt(i);
			if(ch>='a'&&ch<='z') {
				upStr +=(char)(ch-32); 
			}
			else
				upStr += ch;
		}
		System.out.println(upStr);
	}

}
