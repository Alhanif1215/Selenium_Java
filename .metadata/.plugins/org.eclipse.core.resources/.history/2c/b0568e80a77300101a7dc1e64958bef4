package java_basics.program;

public class Emp {
	int id;
	String name;
	double sal;
	public Emp(int id, String name, double sal) {
		//super();
		this.id = id;
		this.name = name;
		this.sal = sal;
	}
	public void display() {
		System.out.print(id);
		System.out.print(name);		
		System.out.print(sal);		
	}
	

}
